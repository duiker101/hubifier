{
	"name": "hubifier",
	"version": "1.2.0",
	"license": "MIT",
	"scripts": {
		"clean": "rimraf build .cache",
		"serve": "parcel src/background.ts src/popup/index.html -d build",
		"build": "yarn run clean && parcel build src/background.ts src/popup/index.html -d build",
		"watch": "parcel watch build src/background.ts src/popup/index.html -d build"
	},
	"devDependencies": {
		"node-sass": "^6.0.1",
		"parcel-bundler": "^1.12.5",
		"parcel-plugin-static-files-copy": "^2.6.0",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"@types/chrome": "^0.0.157",
		"@types/moment": "^2.13.0",
		"axios": "^0.21.4",
		"eslint": "^7.32.0",
		"github": "^14.0.0",
		"moment": "^2.29.4",
		"preact": "^10.5.14",
		"prettier": "^2.4.0",
		"rimraf": "^3.0.2"
	},
	"browserslist": [
		"last 1 Chrome versions"
	]
}
